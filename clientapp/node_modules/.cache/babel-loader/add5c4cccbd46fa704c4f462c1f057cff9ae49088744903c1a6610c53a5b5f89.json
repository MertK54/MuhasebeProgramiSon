wal({title:'Successfuly Deleted'});\r\n                    this.getListInvoice();\r\n                }\r\n                else{\r\n                    console.log(\"error in response\", response)\r\n                    swal({title:'Error Deleted'})\r\n                }\r\n            })\r\n            .catch(error => {\r\n                console.log(\"Error trying to delete:\",error)\r\n                swal({title:\"Error when trying to delete \",icon:\"error\"})\r\n            })\r\n        },\r\n        updateStock(id, quantitySold, unitPrice) {\r\n            const params = {\r\n                stock_id: id,\r\n                quantity: quantitySold,\r\n                unit_price: unitPrice\r\n            }\r\n            console.log(\"params deÄŸerleri:\"+params);\r\n            axios.post('http://localhost:5280/api/stock/stock-update-add',params, {headers: { 'Content-Type': 'application/json' }})\r\n            .then(response => {\r\n                console.log(\"Stock updated\", response.data);\r\n            })\r\n            .catch(error => {\r\n                console.log(\"Error updating stock\", error.response ? error.response.data : error);\r\n                swal({ title: \"Stock update failed\", text: \"Could not update stock\", icon: \"error\" });\r\n            });\r\n          }\r\n    }\r\n}\r\n</script>"],"mappings":";;EACWA,KAAK,EAAC;AAAkB;;EAkBfA,KAAK,EAAC;AAAK;mBAnB/B;;uBACIC,mBAAA,CA+BQ,SA/BRC,UA+BQ,G,0BA9BJC,mBAAA,CAcQ,gBAbJA,mBAAA,CAYK,aAXDA,mBAAA,CAA+B;IAA3BH,KAAK,EAAC;EAAK,GAAC,YAAU,GAC1BG,mBAAA,CAAqB,YAAjB,cAAY,GAChBA,mBAAA,CAAiB,YAAb,UAAQ,GACZA,mBAAA,CAAiB,YAAb,UAAQ,GACZA,mBAAA,CAAgB,YAAZ,SAAO,GACXA,mBAAA,CAAe,YAAX,QAAM,GACVA,mBAAA,CAAa,YAAT,MAAI,GACRA,mBAAA,CAAiB,YAAb,UAAQ,GACZA,mBAAA,CAAmB,YAAf,YAAU,GACdA,mBAAA,CAAqB,YAAjB,cAAY,GAChBA,mBAAA,CAAe,YAAX,QAAM,E,wBAGlBA,mBAAA,CAcQ,iB,kBAbJF,mBAAA,CAYKG,SAAA,QA9BjBC,WAAA,CAkB2CC,KAAA,CAAAC,QAAQ,EAlBnD,CAkBwBC,OAAO,EAAEC,KAAK;yBAA1BR,mBAAA,CAYK;MAZqCS,GAAG,EAAED;IAAK,IAChDN,mBAAA,CAA6C,MAA7CQ