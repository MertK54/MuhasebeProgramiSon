assword: ''\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      axios.post('http://localhost:5280/api/user/login', this.FormData, {\r\n        headers: {\r\n          'Content-Type': 'application/json'\r\n        }\r\n      })\r\n      .then(response => {\r\n        if(response.data.token !== \"\" || response.data.token !== null)\r\n      {\r\n        alert(\"Login successful.\");\r\n        if (response.data.redirectUrl) {\r\n          localStorage.setItem(response.data.token);\r\n          this.$router.push('/dashboard');\r\n        }\r\n      }\r\n      })\r\n      .catch(error => {\r\n        console.error('error: ', error);\r\n        alert('error');\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\nhtml, body {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  height: 100%;\r\n  overflow: hidden;\r\n}\r\n.container, .row {\r\n  height: 100%;\r\n}\r\n\r\n.lead {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.card-body {\r\n  padding: 3rem; \r\n}\r\n\r\nbutton {\r\n  padding: 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXP,KAAK,CAACQ,IAAI,CAAC,sCAAsC,EAAE,IAAI,CAACL,QAAQ,EAAE;QAChEM,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAGA,QAAQ,CAACT,IAAI,CAACU,KAAI,KAAM,EAAC,IAAKD,QAAQ,CAACT,IAAI,CAACU,KAAI,KAAM,IAAI,EAC/D;UACEC,KAAK,CAAC,mBAAmB,CAAC;UAC1B,IAAIF,QAAQ,CAACT,IAAI,CAACY,WAAW,EAAE;YAC7BC,YAAY,CAACC,OAAO,CAACL,QAAQ,CAACT,IAAI,CAACU,KAAK,CAAC;YACzC,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;UACjC;QACF;MACA,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,SAAS,EAAEA,KAAK,CAAC;QAC/BP,KAAK,CAAC,OAAO,CAAC;MAChB,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}