{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Vue from 'vue';\nexport default {\n  name: 'UserLogin',\n  data() {\n    return {\n      FormData: {\n        username: '',\n        password: ''\n      },\n      token: null\n    };\n  },\n  methods: {\n    submitForm() {\n      axios.post('http://localhost:5280/api/user/login', this.FormData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        alert(\"Login successful.\");\n        this.token = response.data.token; // Token'ı sakla\n        Vue.prototype.$token = this.token; // Global token\n        if (response.data.redirectUrl) {\n          this.$router.push(response.data.redirectUrl);\n        }\n      }).catch(error => {\n        console.error('An error occurred while logging in: ', error);\n        alert('An error occurred while logging in.');\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","Vue","name","data","FormData","username","password","token","methods","submitForm","post","headers","then","response","alert","prototype","$token","redirectUrl","$router","push","catch","error","console"],"sources":["C:\\Users\\Polisoft WEB\\Desktop\\MuhasebeProgrami\\MuhasebeProgrami\\clientapp\\src\\views\\UserLogin.vue"],"sourcesContent":["<template>\r\n  <section class=\"login-section\" style=\"background-color: #081028; color: white;\">\r\n    <div class=\"container min-vh-100 d-flex align-items-center justify-content-center\">\r\n      <div class=\"row align-items-center\">\r\n        <div class=\"col-12 col-md-6\">\r\n          <div class=\"welcome-text d-flex flex-column justify-content-center align-items-start\">\r\n            <hr class=\"border-primary-subtle mb-4\">\r\n            <h2 class=\"h1 mb-4\">Welcome to the World of Accounting</h2>\r\n            <p class=\"lead mb-5\">You're in the right place to manage your business's financial health.</p>\r\n            <p class=\"mb-5\" style=\"height: 50px;\"></p>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-12 col-md-6\">\r\n          <div class=\"card border-0 rounded-4\" style=\"background-color: rgb(8, 17, 70); width: 100%;\">\r\n            <div class=\"card-body p-3 p-md-4 m-4\">\r\n              <div class=\"row\">\r\n                <div class=\"col-12\">\r\n                  <div class=\"mb-4\">\r\n                    <h3 style=\"color: white;\">Sign In</h3>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <form @submit.prevent=\"submitForm\">\r\n                <div class=\"row gy-3 overflow-hidden\">\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input v-model=\"FormData.username\" type=\"text\" name=\"username\" id=\"username\" class=\"form-control\" required>\r\n                      <label for=\"username\" class=\"form-label\">Username</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input v-model=\"FormData.password\" type=\"password\" class=\"form-control\" name=\"password\" id=\"password\" required>\r\n                      <label for=\"password\" class=\"form-label\">Password</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"d-grid\">\r\n                      <button class=\"btn btn-lg\" type=\"submit\" style=\"background-color: rgb(8, 17, 90); color:white;\">Log in now</button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Vue from 'vue';\r\nexport default {\r\n  name: 'UserLogin',\r\n  data() {\r\n    return {\r\n      FormData: {\r\n        username: '',\r\n        password: ''\r\n      },\r\n      token: null\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      axios.post('http://localhost:5280/api/user/login', this.FormData, {\r\n        headers: {\r\n          'Content-Type': 'application/json'\r\n        }\r\n      })\r\n      .then(response => {\r\n        alert(\"Login successful.\");\r\n        this.token = response.data.token; // Token'ı sakla\r\n        Vue.prototype.$token = this.token; // Global token\r\n        if (response.data.redirectUrl) {\r\n          this.$router.push(response.data.redirectUrl);\r\n        }\r\n      })\r\n      .catch(error => {\r\n        console.error('An error occurred while logging in: ', error);\r\n        alert('An error occurred while logging in.');\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\nhtml, body {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  height: 100%;\r\n  overflow: hidden;\r\n}\r\n.container, .row {\r\n  height: 100%;\r\n}\r\n\r\n.lead {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.card-body {\r\n  padding: 3rem; \r\n}\r\n\r\nbutton {\r\n  padding: 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,GAAE,MAAO,KAAK;AACrB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ,CAAC;MACDC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXT,KAAK,CAACU,IAAI,CAAC,sCAAsC,EAAE,IAAI,CAACN,QAAQ,EAAE;QAChEO,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChBC,KAAK,CAAC,mBAAmB,CAAC;QAC1B,IAAI,CAACP,KAAI,GAAIM,QAAQ,CAACV,IAAI,CAACI,KAAK,EAAE;QAClCN,GAAG,CAACc,SAAS,CAACC,MAAK,GAAI,IAAI,CAACT,KAAK,EAAE;QACnC,IAAIM,QAAQ,CAACV,IAAI,CAACc,WAAW,EAAE;UAC7B,IAAI,CAACC,OAAO,CAACC,IAAI,CAACN,QAAQ,CAACV,IAAI,CAACc,WAAW,CAAC;QAC9C;MACF,CAAC,EACAG,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sCAAsC,EAAEA,