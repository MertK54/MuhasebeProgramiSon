{"ast":null,"code":"import axios from 'axios';\nimport swal from 'sweetalert';\nexport default {\n  name: \"CustomerCreateComponent\",\n  data() {\n    return {\n      formData: {\n        name: '',\n        e_mail: '',\n        phone_number: null,\n        adress: ''\n      }\n    };\n  },\n  methods: {\n    createCustomer() {\n      var params = {\n        name: this.formData.name,\n        e_mail: this.formData.e_mail,\n        phone_number: this.formData.phone_number.toString(),\n        adress: this.formData.adress\n      };\n      console.log(\"params values\", params);\n      axios.post('http://localhost:5280/api/customer/customer-create', params, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        if (response && response.data && response.data.token.length === 36) {\n          swal({\n            title: \"Succesfully added customer\",\n            icon: \"success\"\n          });\n        } else {\n          swal({\n            title: \"Error added customer\",\n            text: \"there is a customer with this email and name\",\n            icon: \"success\"\n          });\n        }\n      }).catch(error => {\n        swal({\n          title: \"Error added customer\",\n          text: \"there is a customer with this email and name\",\n          icon: \"success\"\n        });\n        console.log(\"error in create customer\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","swal","name","data","formData","e_mail","phone_number","adress","methods","createCustomer","params","toString","console","log","post","headers","then","response","token","length","title","icon","text","catch","error"],"sources":["C:\\Users\\Polisoft WEB\\Documents\\MuhasebeProgrami\\clientapp\\src\\components\\Customers\\CustomerCreateComponent.vue"],"sourcesContent":["<template>\n    <h1>Create Customer</h1>\n    <form @submit.prevent=\"createCustomer\" class=\"mx-auto my-3 p-5\" style=\"width: 50rem;border: 1px solid grey;\">\n      <div class=\"row mb-3\">\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"validationDefault01\" class=\"form-label\">Name</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.name\" required>\n        </div>\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"validationDefault02\" class=\"form-label\">Email</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.e_mail\" required pattern=\"^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$\">\n        </div>\n      </div>\n      <div class=\"row mb-3 mb-2\">\n        <div class=\"col-md-6\">\n          <label for=\"phone\" class=\"form-label\">Phone</label>\n          <input type=\"number\" class=\"form-control\" v-model=\"formData.phone_number\" required  pattern=\"^\\+?[0-9]{7,15}$\">\n        </div>\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"validationDefault04\" class=\"form-label\">Address</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.adress\" required>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-6\">\n          <div class=\"form-check\">\n            <input class=\"form-check-input\" type=\"checkbox\" required>\n            <label class=\"form-check-label\" for=\"invalidCheck2\">\n              Agree to terms and conditions\n            </label>\n          </div>\n        </div>\n        <div class=\"col-6\">\n          <button class=\"btn btn-primary\" type=\"submit\">Create</button>\n        </div>\n      </div>\n    </form>\n  </template>\n  \n  <script>\nimport axios from 'axios';\nimport swal from 'sweetalert';\n  export default {\n    name: \"CustomerCreateComponent\",\n  data(){\n    return{\n        formData:{\n            name:'',\n            e_mail:'',\n            phone_number:null,\n            adress:''\n        }\n    }\n  },\n  methods:{\n    createCustomer(){\n      var params={\n        name:this.formData.name,\n        e_mail:this.formData.e_mail,\n        phone_number:this.formData.phone_number.toString(),\n        adress:this.formData.adress\n      }\n      console.log(\"params values\",params)\n        axios.post('http://localhost:5280/api/customer/customer-create',params,{headers:{'Content-Type':'application/json'}})\n        .then(response => {\n        if (response && response.data && response.data.token.length===36) {\n          swal({title: \"Succesfully added customer\",icon: \"success\"});\n        }\n        else{\n          swal({title: \"Error added customer\",text:\"there is a customer with this email and name\",icon: \"success\"});\n        }\n      })\n        .catch(error => {\n            swal({title: \"Error added customer\",text:\"there is a customer with this email and name\",icon: \"success\"});\n            console.log(\"error in create customer\",error)\n        })\n    }\n  }\n};\n  </script>\n  \n  <style>\n  </style>\n  "],"mappings":"AAwCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,YAAY;AAC3B,eAAe;EACbC,IAAI,EAAE,yBAAyB;EACjCC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACFC,QAAQ,EAAC;QACLF,IAAI,EAAC,EAAE;QACPG,MAAM,EAAC,EAAE;QACTC,YAAY,EAAC,IAAI;QACjBC,MAAM,EAAC;MACX;IACJ;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,cAAcA,CAAA,EAAE;MACd,IAAIC,MAAM,GAAC;QACTR,IAAI,EAAC,IAAI,CAACE,QAAQ,CAACF,IAAI;QACvBG,MAAM,EAAC,IAAI,CAACD,QAAQ,CAACC,MAAM;QAC3BC,YAAY,EAAC,IAAI,CAACF,QAAQ,CAACE,YAAY,CAACK,QAAQ,CAAC,CAAC;QAClDJ,MAAM,EAAC,IAAI,CAACH,QAAQ,CAACG;MACvB;MACAK,OAAO,CAACC,GAAG,CAAC,eAAe,EAACH,MAAM;MAChCV,KAAK,CAACc,IAAI,CAAC,oDAAoD,EAACJ,MAAM,EAAC;QAACK,OAAO,EAAC;UAAC,cAAc,EAAC;QAAkB;MAAC,CAAC,EACnHC,IAAI,CAACC,QAAO,IAAK;QAClB,IAAIA,QAAO,IAAKA,QAAQ,CAACd,IAAG,IAAKc,QAAQ,CAACd,IAAI,CAACe,KAAK,CAACC,MAAM,KAAG,EAAE,EAAE;UAChElB,IAAI,CAAC;YAACmB,KAAK,EAAE,4BAA4B;YAACC,IAAI,EAAE;UAAS,CAAC,CAAC;QAC7D,OACI;UACFpB,IAAI,CAAC;YAACmB,KAAK,EAAE,sBAAsB;YAACE,IAAI,EAAC,8CAA8C;YAACD,IAAI,EAAE;UAAS,CAAC,CAAC;QAC3G;MACF,CAAC,EACEE,KAAK,CAACC,KAAI,IAAK;QACZvB,IAAI,CAAC;UAACmB,KAAK,EAAE,sBAAsB;U