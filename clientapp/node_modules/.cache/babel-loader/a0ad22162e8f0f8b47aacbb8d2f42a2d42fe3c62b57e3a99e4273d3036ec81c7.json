ents\\Customers\\CustomerUpdateComponent.vue"],"sourcesContent":["<template>\n    <table class=\"table table-dark\">\n        <thead>\n            <tr>\n                <th>Name</th>\n                <th>Email</th>\n                <th>Phone</th>\n                <th>Address</th>\n                <th>Update</th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"(customer, index) in customers\" :key=\"index\">\n                <td>{{ customer.name }}</td>\n                <td>{{ customer.e_mail }}</td>\n                <td>{{ customer.phone_number }}</td>\n                <td>{{ customer.adress }}</td>\n                <td><button class=\"btn btn-primary\" @click=\"openUpdateModal(customer)\">Update</button></td>\n            </tr>\n        </tbody>\n    </table>\n    <div class=\"modal fade text-dark\" id=\"updateModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n        <div class=\"modal-dialog\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <h5 class=\"modal-title\">Update Customer</h5>\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                </div>\n                <div class=\"modal-body\">\n                    <form @submit.prevent=\"updateCustomer\">\n                        <div class=\"mb-3\">\n                            <label for=\"name\" class=\"form-label\">Name</label>\n                            <input type=\"text\" v-model=\"selectedCustomer.name\" class=\"form-control\" id=\"name\">\n                        </div>\n                        <div class=\"mb-3\">\n                            <label for=\"email\" class=\"form-label\">Email</label>\n                            <input type=\"email\" v-model=\"selectedCustomer.e_mail\" class=\"form-control\" id=\"email\">\n                        </div>\n                        <div class=\"mb-3\">\n                            <label for=\"phone\" class=\"form-label\">Phone</label>\n                            <input type=\"text\" v-model=\"selectedCustomer.phone_number\" class=\"form-control\" id=\"phone\">\n                        </div>\n                        <div class=\"mb-3\">\n                            <label for=\"address\" class=\"form-label\">Address</label>\n                            <input type=\"text\" v-model=\"selectedCustomer.adress\" class=\"form-control\" id=\"address\">\n                        </div>\n                        <button type=\"submit\" class=\"btn btn-primary\">Save changes</button>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { Modal } from 'bootstrap';\nimport swal from 'sweetalert';\nexport default {\n    name: \"CustomerUpdateComponent\",\n    data() {\n        return {\n            customers: [],\n            selectedCustomer: {}\n        }\n    },\n    mounted() {\n        this.getCustomer();\n    },\n    methods: {\n        getCustomer() {\n            axios.get('http://localhost:5280/api/customer/customer-get')\n                .then(response => {\n                    this.customers = response.data;\n                })\n                .catch(error => {\n                    console.log(\"Customers Not Get\", error);\n                });\n        },\n        openUpdateModal(customer) {\n            this.selectedCustomer = { ...customer };\n            const modal = new Modal(document.getElementById('updateModal'));\n            modal.show();\n        },\n        updateCustomer() {\n            const matched = this.customers\n            axios.post(\"http://localhost:5280/api/customer/customer-update\", this.selectedCustomer)\n                .then(response => {\n                    if(response.data.token.length===36){\n                        swal({title: \"Customer succesfully updated\",icon: \"success\"});\n                        this.getCustomer();\n                        const modal = Modal.getInstance(document.getElementById('updateModal'));\n                        modal.hide();\n                    }\n                })\n                .catch(error => {\n                    console.log(\"Update fail\", error);\n                    swal({title: \"Customer not updated\",text: \"Error when updated customer\",icon: \"warning\",dangerMode: true});\n\n                });\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>\n"],"mappings":"AAuDA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,KAAI,QAAS,WAAW;AACjC,OAAOC,IAAG,MAAO,YAAY;AAC7B,eAAe;EACXC,IAAI,EAAE,yBAAyB;EAC/BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,gBAAgB,EAAE,CAAC;IACvB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,WAAW,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACLD,WAAWA,CAAA,EAAG;MACVR,KAAK,CAACU,GAAG,CAAC,iDAAiD,EACtDC,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACR,IAAI;MAClC,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEF,KAAK,CAAC;MAC3C,CAAC,CAAC;IACV,CAAC;IACDG,eAAeA,CAACC,QAAQ,EAAE;MACtB,IAAI,CAACZ,gBAAe,GAAI;QAAE,GAAGY;MAAS,CAAC;MACvC,MAAMC,KAAI,GAAI,IAAIlB,KAAK,CAACmB,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAAC;MAC/DF,KAAK,CAACG,IAAI,CAAC,CAAC;IAChB,CAAC;IACDC,cAAcA,CAAA,EAAG;MACb,MAAMC,OAAM,GAAI,IAAI,CAACnB,SAAQ;MAC7BL,KAAK,CAACyB,IAAI,CAAC,oDAAoD,EAAE,IAAI,CAACnB,gBAAgB,EACjFK,IAAI,CAACC,QAAO,IAAK;QACd,IAAGA,QAAQ,CAACR,IAAI,CAACsB,KAAK,CAACC,MAAM,KAAG,EAAE,EAAC;UAC/BzB,IAAI,CAAC;YAAC0B,KAAK,EAAE,8BAA8B;YAACC,IAAI,EAAE;UAAS,CAAC,CAAC;UAC7D,IAAI,CAACrB,WAAW,CAAC,CAAC;UAClB,MAAMW,KAAI,GAAIlB,KAAK,CAAC6B,WAAW,CAACV,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAAC;UACvEF,KAAK,CAACY,IAAI,CAAC,CAAC;QAChB;MACJ,CAAC,EACAlB,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAAC,aAAa,EAAEF,KAAK,CAAC;QACjCZ,IAAI,CAAC;UAAC0B,KAAK,EAAE,sBAAsB;UAACI,IAAI,EAAE,6BAA6B;UAACH,IAAI,EAAE,SAAS;UAACI,UAAU,EAAE;QAAI,CAAC,CAAC;MAE9G,CAAC,CAAC;IACV;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType