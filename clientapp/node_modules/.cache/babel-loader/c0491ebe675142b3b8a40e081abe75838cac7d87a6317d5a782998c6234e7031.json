ct/dist/vue-select.css';\nexport default {\n  name: \"StockCreateComponent\",\n  components:{\n    vSelect,\n  },\n    data(){\n        return {\n            formData:{\n                product_name:'',\n                quantity:'',\n                unit_price:'',\n            },\n            supplier:[],\n            data:[]\n        }\n    },\n    created(){\n        this.getSupplier();\n    },\n    methods:{\n        getSupplier(){\n            axios.get('http://localhost:5280/api/supplier/supplier-get')\n            .then(response => {\n                if(response != null && response.data != null){\n                  this.data = Array.isArray(response.data) ? response.data : [];\n                  this.supplier = [...new Set(this.data.map(item => item.name))];\n                }\n            })\n        },\n        createStock(){\n          const params = {\n            name: this.formData.product_name,\n            e_mail: this.formData.quantity,\n            phone_number:this.formData.unit_price,\n          }\n            axios.post('http://localhost:5280/api/stock/stock-create',params,{headers:{'Content-Type':'application/json'}})\n            .then(response => {\n                console.log(\"Stock created\",response.data)\n            })\n            .catch(error => {\n                console.log(\"Error creating stock\",error)\n            })\n        }\n    }\n}\n</script>\n<style>\n</style>"],"mappings":";;;;;;;AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,YAAY;AAChC,OAAO,gCAAgC;AACvC,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAC;IACTF;EACF,CAAC;EACCG,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,QAAQ,EAAC;QACLC,YAAY,EAAC,EAAE;QACfC,QAAQ,EAAC,EAAE;QACXC,UAAU,EAAC;MACf,CAAC;MACDC,QAAQ,EAAC,EAAE;MACXL,IAAI,EAAC;IACT;EACJ,CAAC;EACDM,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,WAAW,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAC;IACJD,WAAWA,CAAA,EAAE;MACTX,KAAK,CAACa,GAAG,CAAC,iDAAiD,EAC1DC,IAAI,CAACC,QAAO,IAAK;QACd,IAAGA,QAAO,IAAK,IAAG,IAAKA,QAAQ,CAACX,IAAG,IAAK,IAAI,EAAC;UAC3C,IAAI,CAACA,IA