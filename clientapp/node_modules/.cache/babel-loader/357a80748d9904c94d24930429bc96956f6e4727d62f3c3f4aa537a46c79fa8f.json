{"ast":null,"code":"import axios from 'axios';\nimport vSelect from 'vue-select';\nimport 'vue-select/dist/vue-select.css';\nimport swal from 'sweetalert';\nexport default {\n  name: \"StockCreateComponent\",\n  components: {\n    vSelect\n  },\n  data() {\n    return {\n      formData: {\n        supplier_name: '',\n        supplier_id: null,\n        product_name: '',\n        quantity: '',\n        unit_price: ''\n      },\n      supplier: [],\n      data: []\n    };\n  },\n  created() {\n    this.getSupplier();\n  },\n  watch: {\n    'formData.supplier_id'(newValue) {\n      console.log(newVal);\n      this.formData.supplier_name = this.supplier.find(supplier => supplier.supplier_id === newValue.supplier_id)?.name;\n    }\n  },\n  methods: {\n    getSupplier() {\n      axios.get('http://localhost:5280/api/supplier/supplier-get').then(response => {\n        if (response && response.data) {\n          this.supplier = response.data.map(item => ({\n            id: item.supplier_id,\n            name: item.name\n          }));\n        }\n      });\n    },\n    createStock() {\n      const params = {\n        supplier_id: this.formData.supplier_id,\n        supplier_name: this.formData.supplier_name,\n        product_name: this.formData.product_name,\n        quantity: parseInt(this.formData.quantity, 10),\n        unit_price: parseFloat(this.formData.unit_price)\n      };\n      console.log(\"params values\", params);\n      axios.post('http://localhost:5280/api/stock/stock-create', params, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        swal({\n          title: \"Stocks created\",\n          icon: \"success\"\n        });\n        console.log(\"Stock created\", response.data);\n      }).catch(error => {\n        console.log(\"Error creating stock\", error.response ? error.response.data : error); // Hata mesajını detaylı göster\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","vSelect","swal","name","components","data","formData","supplier_name","supplier_id","product_name","quantity","unit_price","supplier","created","getSupplier","watch","formData.supplier_id","newValue","console","log","newVal","find","methods","get","then","response","map","item","id","createStock","params","parseInt","parseFloat","post","headers","title","icon","catch","error"],"sources":["C:\\Users\\Polisoft WEB\\Documents\\MuhasebeProgrami\\clientapp\\src\\components\\Stocks\\StockCreateComponent.vue"],"sourcesContent":["<template>\n    <h1>Stock Supplier</h1>\n    <form @submit.prevent=\"createStock\" class=\"mx-auto my-3 p-5 bg-dark\" style=\"width: 50rem;border: 1px solid grey;\">\n      <div class=\"row mb-3\">\n        <div class=\"col-md-12 mb-2\">\n          <label for=\"validationDefault01\" class=\"form-label\">Product Name</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.product_name\" required>\n        </div>\n        <label for=\"supplier\" class=\"form-label\">Select Supplier:</label>\n        <div class=\"row mb-3\" style=\"margin-left:0px;padding: 0px;\">\n            <v-select :options=\"supplier\" label=\"name\" v-model=\"formData.supplier_id\" :reduce=\"supplier => supplier.id\" placeholder=\"Select Supplier\" style=\"color:black\"></v-select>\n        </div>\n        <div class=\"col-md-12 mb-2\">\n          <label for=\"validationDefault02\" class=\"form-label\">Quantity</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.quantity\" required>\n        </div>\n      </div>\n      <div class=\"row mb-3 mb-2\">\n        <div class=\"col-md-12\">\n          <label for=\"validationDefault03\" class=\"form-label\">Unit Price</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.unit_price\" required>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-6\">\n          <div class=\"form-check\">\n            <input class=\"form-check-input\" type=\"checkbox\" required>\n            <label class=\"form-check-label\" for=\"invalidCheck2\">\n              Agree to terms and conditions\n            </label>\n          </div>\n        </div>\n        <div class=\"col-12 mt-2\">\n          <button class=\"btn btn-primary\" type=\"submit\">Create</button>\n        </div>\n      </div>\n    </form>\n  </template>\n<script>\nimport axios from 'axios';\nimport vSelect from 'vue-select';\nimport 'vue-select/dist/vue-select.css';\nimport swal from 'sweetalert';\nexport default {\n  name: \"StockCreateComponent\",\n  components:{\n    vSelect,\n  },\n    data(){\n        return {\n            formData:{\n                supplier_name:'',\n                supplier_id: null,\n                product_name:'',\n                quantity:'',\n                unit_price:''\n            },\n            supplier:[],\n            data:[]\n        }\n    },\n    created(){\n        this.getSupplier();\n    },\n    watch:{\n      'formData.supplier_id'(newValue){\n        console.log(newVal)\n        this.formData.supplier_name = this.supplier.find(supplier => supplier.supplier_id === newValue.supplier_id)?.name\n      }\n    },\n    methods:{\n        getSupplier(){\n            axios.get('http://localhost:5280/api/supplier/supplier-get')\n            .then(response => {\n              if (response && response.data) {\n                this.supplier = response.data.map(item => ({ id: item.supplier_id, name: item.name }));\n              }\n            });\n        },\n        createStock() {\n            const params = {\n              supplier_id: this.formData.supplier_id,\n              supplier_name: this.formData.supplier_name,\n              product_name: this.formData.product_name,\n              quantity: parseInt(this.formData.quantity, 10), \n              unit_price: parseFloat(this.formData.unit_price), \n            };\n            console.log(\"params values\",params);\n            axios.post('http://localhost:5280/api/stock/stock-create', params, {\n                headers: { 'Content-Type': 'application/json' }\n            })\n            .then(response => {\n              swal({title:\"Stocks created\",icon:\"success\"})\n                console.log(\"Stock created\", response.data);\n            })\n            .catch(error => {\n                console.log(\"Error creating stock\", error.response ? error.response.data : error); // Hata mesajını detaylı göster\n            });\n        }\n    }\n}\n</script>\n<style>\n.vs__dropdown-toggle {\n  background-color: white!important; /* Seçeneklerin arka plan rengini beyaz yap */\n  color: black!important; /* Seçeneklerin metin rengini siyah yap */\n  width: 100%;\n}\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,YAAY;AAChC,OAAO,gCAAgC;AACvC,OAAOC,IAAG,MAAO,YAAY;AAC7B,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAC;IACTH;EACF,CAAC;EACCI,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,QAAQ,EAAC;QACLC,aAAa,EAAC,EAAE;QAChBC,WAAW,EAAE,IAAI;QACjBC,YAAY,EAAC,EAAE;QACfC,QAAQ,EAAC,EAAE;QACXC,UAAU,EAAC;MACf,CAAC;MACDC,QAAQ,EAAC,EAAE;MACXP,IAAI,EAAC;IACT;EACJ,CAAC;EACDQ,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,WAAW,CAAC,CAAC;EACtB,CAAC;EACDC,KAAK,EAAC;IACJ,sBAAsBC,CAACC,QAAQ,EAAC;MAC9BC,OAAO,CAACC,GAAG,CAACC,MAAM;MAClB,IAAI,CAACd,QAAQ,CAACC,aAAY,GAAI,IAAI,CAACK,QAAQ,CAACS,IAAI,CAACT,QAAO,IAAKA,QAAQ,CAACJ,WAAU,KAAMS,QAAQ,CAACT,WAAW,CAAC,EAAEL,IAAG;IAClH;EACF,CAAC;EACDmB,OAAO,EAAC;IACJR,WAAWA,CAAA,EAAE;MACTd,KAAK,CAACuB,GAAG,CAAC,iDAAiD,EAC1DC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAO,IAAKA,QAAQ,CAACpB,IAAI,EAAE;UAC7B,IAAI,CAACO,QAAO,GAAIa,QAAQ,CAACpB,IAAI,CAACqB,GAAG,CAACC,IAAG,KAAM;YAAEC,EAAE,EAAED,IAAI,CAACnB,WAAW;YAAEL,IAAI,EAAEwB,IAAI,CAACxB;UAAK,CAAC,CAAC,CAAC;QACxF;MACF,CAAC,CAAC;IACN,CAAC;IACD0B,WAAWA,CAAA,EAAG;MACV,MAAMC,MAAK,GAAI;QACbtB,WAAW,EAAE,IAAI,CAACF,QAAQ,CAACE,WAAW;QACtCD,aAAa,EAAE,IAAI,CAACD,QAAQ,CAACC,aAAa;QAC1CE,YAAY,EAAE,IAAI,CAACH,QAAQ,CAACG,YAAY;QACxCC,QAAQ,EAAEqB,QAAQ,CAAC,IAAI,CAACzB,QAAQ,CAACI,QAAQ,EAAE,EAAE,CAAC