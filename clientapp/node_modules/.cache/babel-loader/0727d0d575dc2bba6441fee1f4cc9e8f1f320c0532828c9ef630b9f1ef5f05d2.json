   <div class=\"col-12 col-md-6\">\n          <div class=\"card border-0 rounded-4\" style=\"background-color: rgb(8, 17, 70); width: 100%;\">\n            <div class=\"card-body p-3 p-md-4 m-4\">\n              <div class=\"row\">\n                <div class=\"col-12\">\n                  <div class=\"mb-4\">\n                    <h3 style=\"color: white;\">Sign In</h3>\n                  </div>\n                </div>\n              </div>\n              <!-- Form section with added spacing and height -->\n              <form @submit.prevent=\"submitForm\">\n                <div class=\"row gy-3 overflow-hidden\">\n                  <div class=\"col-12\">\n                    <div class=\"form-floating mb-3\">\n                      <input v-model=\"FormData.username\" type=\"text\" name=\"username\" id=\"username\" class=\"form-control\" required>\n                      <label for=\"username\" class=\"form-label\">Username</label>\n                    </div>\n                  </div>\n                  <div class=\"col-12\">\n                    <div class=\"form-floating mb-3\">\n                      <input v-model=\"FormData.password\" type=\"password\" class=\"form-control\" name=\"password\" id=\"password\" required>\n                      <label for=\"password\" class=\"form-label\">Password</label>\n                    </div>\n                  </div>\n                  <div class=\"col-12\">\n                    <div class=\"d-grid\">\n                      <button class=\"btn btn-lg\" type=\"submit\" style=\"background-color: rgb(8, 17, 90); color:white;\">Log in now</button>\n                    </div>\n                  </div>\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'UserLogin',\n  data() {\n    return {\n      FormData: {\n        username: '',\n        password: ''\n      }\n    };\n  },\n  methods: {\n    submitForm() {\n      axios.post('http://localhost:5280/api/user/login', this.FormData, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      })\n      .then(response => {\n        alert(\"Data sent successfully.\");\n        localStorage.setItem('token', response.data.token);\n        if (response.data.redirectUrl) {\n          this.$router.push(response.data.redirectUrl);\n        }\n      })\n      .catch(error => {\n        console.error('An error occurred while sending the data: ', error);\n        alert('An error occurred while sending the data.');\n      });\n    }\n  }\n};\n</script>\n\n<style>\nhtml, body {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  height: 100%;\n  overflow: hidden;\n}\n.container, .row {\n  height: 100%;\n}\n\n.lead {\n  margin-bottom: 2rem; /* Adjusted spacing */\n}\n\n.card-body {\n  padding: 3rem; /* Increased padding for more height */\n}\n\nbutton {\n  padding: 1rem; /* Increased button size */\n}\n</style>\n"],"mappings":";AAwDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXP,KAAK,CAACQ,IAAI,CAAC,sCAAsC,EAAE,IAAI,CAACL,QAAQ,EAAE;QAChEM,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChBC,KAAK,CAAC,yBAAyB,CAAC;QAChCC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACT,IAAI,CAACa,KAAK,CAAC;QAClD,IAAIJ,QAAQ,CAACT,IAAI,CAACc,WAAW,EAAE;UAC7B,IAAI,CAACC,OAAO,CAACC,IAAI,CAACP,QAAQ,CAACT,IAAI,CAACc,WAAW,CAAC;QAC9C;MACF,CAAC,EACAG,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,4CAA4C,EAAEA,KAAK,CAAC;QAClER,KAAK,CAAC,2CAA2C,CAAC;MACpD,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}