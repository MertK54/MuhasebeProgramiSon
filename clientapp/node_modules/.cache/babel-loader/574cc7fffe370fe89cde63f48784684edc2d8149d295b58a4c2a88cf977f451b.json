{"ast":null,"code":"import axios from 'axios';\nimport swal from 'sweetalert';\nexport default {\n  name: \"SupplierCreateComponent\",\n  data() {\n    return {\n      formData: {\n        name: '',\n        e_mail: '',\n        phone_number: '',\n        adress: ''\n      }\n    };\n  },\n  methods: {\n    createSupplier() {\n      const params = {\n        name: this.formData.name,\n        e_mail: this.formData.e_mail,\n        phone_number: this.formData.phone_number.toString(),\n        adress: this.formData.adress\n      };\n      axios.post('http://localhost:5280/api/supplier/supplier-create', params, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        if (response && response.data && response.data.token.length === 36) {\n          swal({\n            title: \"Succesfully added supplier\",\n            icon: \"success\"\n          });\n        } else {\n          swal({\n            title: \"Succesfully added supplier\",\n            icon: \"success\"\n          });\n        }\n      }).catch(error => {\n        console.log(\"Error creating supplier\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","swal","name","data","formData","e_mail","phone_number","adress","methods","createSupplier","params","toString","post","headers","then","response","token","length","title","icon","catch","error","console","log"],"sources":["C:\\Users\\Polisoft WEB\\Documents\\MuhasebeProgrami\\clientapp\\src\\components\\Suppliers\\SupplierCreateComponent.vue"],"sourcesContent":["<template>\n    <h1>Create Supplier</h1>\n    <form @submit.prevent=\"createSupplier\" class=\"mx-auto my-3 p-5 form-dark\" style=\"width: 50rem;border: 1px solid grey;\">\n      <div class=\"row mb-3\">\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"name\" class=\"form-label\">Name</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.name\" required>\n        </div>\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"email\" class=\"form-label\">Email</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.e_mail\" required pattern=\"^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$\">\n        </div>\n      </div>\n      <div class=\"row mb-3 mb-2\">\n        <div class=\"col-md-6\">\n          <label for=\"phone\" class=\"form-label\">Phone</label>\n          <input type=\"number\" class=\"form-control\" v-model=\"formData.phone_number\" required pattern=\"^\\+?[0-9]{7,15}$\">\n        </div>\n        <div class=\"col-md-6 mb-2\">\n          <label for=\"adress\" class=\"form-label\">Address</label>\n          <input type=\"text\" class=\"form-control\" v-model=\"formData.adress\" required>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-6\">\n          <div class=\"form-check\">\n            <input class=\"form-check-input\" type=\"checkbox\" required>\n            <label class=\"form-check-label\" for=\"invalidCheck2\">\n              Agree to terms and conditions\n            </label>\n          </div>\n        </div>\n        <div class=\"col-6\">\n          <button class=\"btn btn-primary\" type=\"submit\">Create</button>\n        </div>\n      </div>\n    </form>\n  </template>\n<script>\nimport axios from 'axios';\nimport swal from 'sweetalert';\nexport default {\n  name: \"SupplierCreateComponent\",\n    data(){\n        return {\n            formData:{\n                name:'',\n                e_mail:'',\n                phone_number:'',\n                adress:''\n            }\n        }\n    },\n    methods:{\n        createSupplier(){\n          const params = {\n            name: this.formData.name,\n            e_mail: this.formData.e_mail,\n            phone_number:this.formData.phone_number.toString(),\n            adress: this.formData.adress\n          }\n            axios.post('http://localhost:5280/api/supplier/supplier-create',params,{headers:{'Content-Type':'application/json'}})\n            .then(response => {\n              if (response && response.data && response.data.token.length===36) {\n                swal({title: \"Succesfully added supplier\",icon: \"success\"});\n              }\n              else{\n                swal({title: \"Succesfully added supplier\",icon: \"success\"});\n\n              }\n            })\n            .catch(error => {\n                console.log(\"Error creating supplier\",error)\n            })\n        }\n    }\n}\n</script>\n<style>\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,YAAY;AAC7B,eAAe;EACbC,IAAI,EAAE,yBAAyB;EAC7BC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,QAAQ,EAAC;QACLF,IAAI,EAAC,EAAE;QACPG,MAAM,EAAC,EAAE;QACTC,YAAY,EAAC,EAAE;QACfC,MAAM,EAAC;MACX;IACJ;EACJ,CAAC;EACDC,OAAO,EAAC;IACJC,cAAcA,CAAA,EAAE;MACd,MAAMC,MAAK,GAAI;QACbR,IAAI,EAAE,IAAI,CAACE,QAAQ,CAACF,IAAI;QACxBG,MAAM,EAAE,IAAI,CAACD,QAAQ,CAACC,MAAM;QAC5BC,YAAY,EAAC,IAAI,CAACF,QAAQ,CAACE,YAAY,CAACK,QAAQ,CAAC,CAAC;QAClDJ,MAAM,EAAE,IAAI,CAACH,QAAQ,CAACG;MACxB;MACEP,KAAK,CAACY,IAAI,CAAC,oDAAoD,EAACF,MAAM,EAAC;QAACG,OAAO,EAAC;UAAC,cAAc,EAAC;QAAkB;MAAC,CAAC,EACnHC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAO,IAAKA,QAAQ,CAACZ,IAAG,IAAKY,QAAQ,CAACZ,IAAI,CAACa,KAAK,CAACC,MAAM,KAAG,EAAE,EAAE;UAChEhB,IAAI,CAAC;YAACiB,KAAK,EAAE,4BAA4B;YAACC,IAAI,EAAE;UAAS,CAAC,CAAC;QAC7D,OACI;UACFlB,IAAI,CAAC;YAACiB,KAAK,EAAE,4BAA4B;YAACC,IAAI,EAAE;UAAS,CAAC,CAAC;QAE7D;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAACF,KAAK;MAC/C,CAAC;IACL;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}